[Unit]
Description=Mission Hollywood - Puzzle 2 (World Map)
After=network.target

[Service]
Type=simple
User=pi
WorkingDirectory=/home/pi/hollywoodHQ/puzzle-2-world-map
ExecStart=/usr/bin/npm start
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

# Environment
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target
